{
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.1",
    "codecov": "^3.6.1",
    "jest": "^24.9.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "tsc:build": "./node_modules/.bin/tsc --build tsconfig.json",
    "tsc:check": "./node_modules/.bin/tsc",
    "typedoc:build": "./node_modules/.bin/typedoc src/",
    "jest:run": "./node_modules/.bin/jest --config jestconfig.json",
    "nyc:test": "./node_modules/.bin/nyc yarn jest:run",
    "nyc:html": "./node_modules/.bin/nyc --reporter=html yarn jest:run",
    "nyc:lcov": "./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov",
    "build": "yarn tsc:build && yarn typedoc:build",
    "test": "yarn tsc:check && yarn nyc:html",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test"
  },
  "name": "argo-navis",
  "version": "0.1.0",
  "description": "a simple Node HTTP framework",
  "main": "index.js",
  "repository": "https://github.com/LXSMNSYC/argo-navis.git",
  "author": "Alexis Munsayac <alexis.munsayac@gmail.com>",
  "license": "MIT",
  "private": false
}
